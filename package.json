  "devDependencies": {},
  "dependencies": {
    "axios": "^0.18.0",
    "concurrently": "^4.1.0",
    "express": "^4.16.4",
    "if-env": "^1.0.4",
    "mongoose": "^5.5.1",
    "nodemon": "^1.18.11"
  }
}
